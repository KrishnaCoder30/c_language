#include <stdio.h>

int main ()
{
    char a[10000],b[1000];
    int i,n=0;
    printf("Input String 1 :-");
    gets (a);
    printf("\nInput String 2 :-");
    gets(b);

    for ( i=0 ; a[i]!='\0' || b[i]!= '\0' ; i++)
    { 
        if(a[i]!= b[i])
        {
            n++;
        }
    }
     if(n==0)
     {
         printf("String are Equal");
     }
     else 
     {
         printf("String are not Equal");
     }
}







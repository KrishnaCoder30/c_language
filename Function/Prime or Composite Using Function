#include <stdio.h>

int Prime(int N)

{

	int i,p,k=0;	for(i=2;i<N;i++)

	{

		if(N%i==0)

		{

			k++;

		}

	}

	if(k==0)

	{

		p=0;

		return p;

	}

	else

	{

		p=1;

		return p;

	}

}

int main()

{

	int N,p;

	printf("Progrm to check Whether a Number is Prime or Not\n");

	

	scanf("%d",&N);

	printf("\nInput Number :- %d\n",N);

	p=Prime(N);

	

	if(p==0)

	{

		printf("\nNumber Inputed is Prime");

	}

	else

	{

		printf("\nNumber Inputed is Composite");

	}

	

}

 
